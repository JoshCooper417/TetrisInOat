class A {
  int i;
  new () () { this.i = 0; }
};

class B <: A {
  new () () { this.i = 1; }
};

class C <: A {
  new () () { this.i = 2; }
};

int program (int argc, string[] argv) {
  A[] arr = {new A(), new B(), new C()};
  for (int i = 0; i < length_of_array (arr); i=i+1;) {
    print_int (arr[i].i);
  }
  return 0;
}
